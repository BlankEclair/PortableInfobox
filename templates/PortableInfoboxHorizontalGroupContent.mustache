<table class="portable-infobox-horizontal-group-content">
	<caption class="portable-infobox-header portable-infobox-secondary-font
	portable-infobox-secondary-background">{{header}}</caption>
	<thead>
		<tr>
			{{#labels}}
				<th class="portable-infobox-horizontal-group-item portable-infobox-item-label
				portable-infobox-secondary-font">{{{.}}}</th>
			{{/labels}}
		</tr>
	</thead>
	<tbody>
	<tr>
		{{#values}}
			<td class="portable-infobox-horizontal-group-item portable-infobox-item-value">{{{.}}}</td>
		{{/values}}
	</tr>
	</tbody>
</table>
